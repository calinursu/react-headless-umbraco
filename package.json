{
  "name": "react-headless-umbraco",
  "author": "@calinursu",
  "license": "MIT",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "^16.x"
  },
  "overrides": {
    "react": "$react",
    "framer-motion": "$framer-motion"
  },
  "scripts": {
    "bs": "next build && next start -p 3005",
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext js,ts,tsx",
    "storybook:build": "build-storybook -s ./public",
    "storybook": "start-storybook -p 6006",
    "storybook:deploy": "node ./tools/deploy-storybook.js",
    "svg": "npx @svgr/cli --ref --typescript --svg-props data-icon --ignore-existing --icon -d ./components/icons ./assets/svgs",
    "test-all": "npm run lint && npm run type-check && npm run test",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watchAll",
    "test": "jest --verbose",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "npm run lint",
      "pretty-quick --staged --pattern ./**/*.{ts,tsx,js,scss}"
    ]
  },
  "dependencies": {
    "@webcomponents/shadydom": "^1.9.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "next": "^12.1.6",
    "next-iron-session": "^4.1.11",
    "next-transpile-modules": "^9.0.0",
    "proxy-polyfill": "^0.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.52.1",
    "swr": "2.1.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@dtsgenerator/replace-namespace": "^1.5.2",
    "@next/eslint-plugin-next": "^12.1.6",
    "@svgr/cli": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.2.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/theme-ui": "^0.3.7",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "dtsgenerator": "^3.15.1",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "ftp-deploy": "^2.4.3",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^1.1.0",
    "lint-staged": "^12.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "replace-in-file": "^6.3.2",
    "sass-loader": "^13.0.0",
    "type-fest": "^2.13.0",
    "typescript": "^4.7",
    "webpack": "^5.75.0",
    "webpack-subresource-integrity": "^5.1.0"
  }
}
